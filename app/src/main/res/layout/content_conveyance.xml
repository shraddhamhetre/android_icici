<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.youtility.intelliwiz20.Activities.ConveyanceActivity"
    android:background="@color/colorWhite"
    tools:showIn="@layout/activity_conveyance"
    android:orientation="vertical"
    android:weightSum="1">
    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="0.20"
        android:visibility="visible">
        <LinearLayout
            android:id="@+id/startTravelLinearLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:orientation="vertical"
            android:visibility="visible"
            android:weightSum="1">



            <TextView
                android:id="@+id/startGpsAddress"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.25"
                android:background="#abe7ed"
                android:gravity="center"
                android:drawableEnd="@android:drawable/stat_notify_sync"
                android:lines="5"
                android:padding="@dimen/text_margin2dp"
                android:text="" />

            <EditText
                android:id="@+id/startGpsAddressManually"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.25"
                android:hint="@string/expence_startlocaddress_hint"
                android:enabled="false"
                android:visibility="visible" />

            <GridView
                android:id="@+id/travelModeGridview"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.50"
                android:horizontalSpacing="5dp"
                android:numColumns="4"
                android:layout_gravity="center"
                android:gravity="center"
                android:scrollbars="none"
                android:stretchMode="columnWidth"
                android:verticalSpacing="5dp"
                android:visibility="visible" />

            <Spinner
                android:id="@+id/travelModeSpinner"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="0.25"
                android:visibility="gone" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/endTravelLinearLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:orientation="vertical"
            android:weightSum="1"
            android:visibility="gone">

            <TextView
                android:id="@+id/endGpsAddress"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.50"
                android:background="#f3a5aa"
                android:drawableEnd="@android:drawable/stat_notify_sync"
                android:lines="5"
                android:gravity="center"
                android:padding="@dimen/text_margin2dp"
                android:text="" />

            <EditText
                android:id="@+id/endGpsAddressManually"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/expence_endlocaddress_hint"
                android:layout_weight="0.10"
                android:layout_gravity="center"
                android:textColor="@color/colorBlack"
                android:enabled="false"
                android:visibility="visible" />

            <EditText
                android:id="@+id/travelExpenceAmount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/expence_amout_hint"
                android:layout_weight="0.05"
                android:layout_gravity="center"
                android:textColor="@color/colorBlack"
                android:inputType="numberDecimal" />
        </LinearLayout>

    </FrameLayout>

    <!--<View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/listviewDivider"/>-->

    <LinearLayout
